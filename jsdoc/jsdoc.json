{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs",
        "template": "./custom-template",
        "tutorials": "./tutorials"
    }
}
